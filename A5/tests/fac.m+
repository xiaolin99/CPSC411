/*  
      To compute the factorial of a number
 */

         var x:real;
         fun fac(a:real):real
         { var z:real;
           begin
              if a=0.0 then z:= 1.0
              else z:= a * fac(a-1.0);
              return z;
           end };
         begin 
            read x;
            print fac(x);
         end
